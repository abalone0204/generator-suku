{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "A simple server for newbie or developming component",
  "main": "src/<%= entry %>",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "clean": "rimraf static/*.js",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "commit": "git-cz"
  },
  "keywords": [
    "react", "webpack"
  ],
  "author": "tjku",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.18.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "node-sass": "^3.3.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.11.0",
    "webpack-hot-middleware": "^2.2.1",
    "commitizen": "^2.4.4",
    "cz-conventional-changelog": "^1.1.4"
  },
  "dependencies": {
    "jquery": "^2.1.4",
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
    "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}